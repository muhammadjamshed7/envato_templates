#: includes/payfast/payfast.php:107 includes/paypal/paypal.php:103
msgid "Gateway disabled"
msgstr "Brama wyłączona"

#: includes/payfast/payfast.php:107
msgid "PayFast does not support your store currency."
msgstr "PayFast nie obsługuje Twojej waluty sklepu."

#: includes/payfast/payfast.php:124
